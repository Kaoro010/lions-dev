function calculadora(valor1, valor2, operacao) {
    switch(operacao){
        case '+':
            valor1 + valor2
        case '-':
            valor1 - valor2
        case '*': 
            valor1 * valor2
        case '/': 
            valor1 / valor2
    }
}

console.log(calculadora(1935, 1945, '+'))
console.log(calculadora(5, 3, '-'))
console.log(calculadora(5, 3, '*'))
console.log(calculadora(5, 3, '/'))